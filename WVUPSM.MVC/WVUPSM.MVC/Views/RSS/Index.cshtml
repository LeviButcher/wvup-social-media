@model List<RssFeedItemViewModel>

<header>
    <h1>RSS Feeds</h1>
</header>
<nav>
    <ul class="profile-nav">
        <li><a asp-route-tab="">Chronicle Articles</a></li>
        <li><a asp-route-tab="WVUP">WVU-P Articles</a></li>
    </ul>
</nav>

@foreach(RssFeedItemViewModel item in Model)
{
    <article class="user-list-item">
        <header>
            @if (ViewData["tab"].ToString() == "")
            {
                <img class="rss-thumb" src="@item.ThumbnailUrl" />
            }
            <div class="rss-content">
                <h3>@item.Title</h3>
                <h6>From: @item.Author, Published On: @item.PubDate</h6>
                <h5>@item.Description</h5>
            </div>
        </header>
    </article>
    <br />
}

