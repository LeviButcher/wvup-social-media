@model List<UserProfile>

<section>
    <header>
        <h1>@ViewData["Title"]</h1>
    </header>
    @if (Model.Count() != 0)
    {
        @foreach (UserProfile user in Model)
        {
            <article>
                <header>
                    <h3>@user.UserName</h3>
                    <h5>@user.Email</h5>
                </header>
                @*Follow Button Here*@
            </article>
        }
    }
    else
    {
        <h2>Nothing to be seen here</h2>
    }
</section>